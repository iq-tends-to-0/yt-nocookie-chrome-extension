{
    "name":"toutube-nocookie",
    "version":"0.2",
    "manifest_version":3,    
    "action": {
        "default_popup":"popup.html"
    },
    "permissions":[
        "activeTab",
        "tabs",
        "scripting"
    ],
    "content_scripts":[
        {
            "matches": ["https://www.youtube-nocookie.com/embed/*"],
            "css":["grayscale.css"]
        }
    ]
}